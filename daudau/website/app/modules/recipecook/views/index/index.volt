<script type="text/javascript">
    var auth ={{ auth_site_home['id'] }};
    var point_rate;
    $("#rateit10").bind('rated', function (event, value) {
        point_rate = value;
        if (auth == null || auth == "") {
            location.href = '/dang-nhap';
            return false;
        } else {
            $.ajax({
                type: 'Post',
                url: "{{ url.get() }}admin/recipe-cook/ajaxRatingRecipe",
                data: {
                    point_rate: point_rate,
                    user: {{ auth_site_home['id'] }},
                    recipe_cook_id: {{ recipe_cook.getId() }},
                },
                dataType: 'json',
                complete: function (data) {
                    location.reload();
                }
            });
        }

    });

    $(document).ready(function () {
        $('#rateit-reset-2').css('display', 'none');
    });
    $(document).on('keydown', '.respone-comment-post', function (e) {
        var nametx = $(this).attr('name');
        console.log($("[name=" + nametx + "]").parent().parent().parent().children('#list_respone_comment').attr('id'));
        if (e.which == 13) {
            if (auth == null || auth == "") {
                location.href = '/dang-nhap';
                return false;
            } else {
                $.ajax({
                    type: 'Post',
                    url: '/admin/comment/postAjaxComment',
                    data: {
                        comment: $(this).val(),
                        type: "respone comment",
